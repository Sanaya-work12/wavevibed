{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuotes": true,
      "trailingComma": "none",
      "bracketSpacing": true,
      "semi": true,
      "useTabs": true,
      "parser": "flow"
    }],
    "no-console": 1
  }
}
